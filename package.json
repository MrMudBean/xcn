{
  "name": "xcn",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "prepublishOnly": "pjj",
    "eslint": "jja cls && eslint src",
    "beautify": "pnpm run prettier",
    "prettier": "jja cls && prettier . --write",
    "diff": "jja pkg -d=淘宝",
    "vjj": "vjj",
    "prepare": "husky",
    "push:version": "gvv",
    "push": "gvv",
    "test": "rollup --config rollup.config.eg.js && node .eg/index.mjs",
    "clean:package": "node scripts/clean-package-json.js",
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "build": "jja rm dist && pnpm run b && pnpm run clean:package"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,jex,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@qqi/rollup-external": "^1.0.11",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.0.3",
    "a-node-tools": "^4.2.20",
    "color-pen": "^3.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.5.0",
    "globals": "^16.5.0",
    "gvv": "^0.1.8",
    "husky": "^9.1.7",
    "jja": "^2.3.21",
    "lint-staged": "^16.2.7",
    "pjj": "^1.0.4",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vjj": "^1.0.11"
  },
  "dependencies": {
    "a-type-of-js": "^2.0.0"
  }
